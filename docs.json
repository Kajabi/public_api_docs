{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Kajabi",
  "colors": {
    "primary": "#ff757a",
    "light": "#fe7279",
    "dark": "#ff757a"
  },
  "fonts": {
    "body": {
      "family": "Faire Sprig",
      "source": "https://cdn.prod.website-files.com/66aebc00ad11d5f27673a187/66aebc00ad11d5f27673a25f_FAIRE-Sprig-Regular.woff2",
      "format": "woff2"
    },
    "heading": {
      "family": "Greet",
      "source": "https://cdn.prod.website-files.com/66aebc00ad11d5f27673a187/66aebc00ad11d5f27673a1f0_Greet%20VF.woff2",
      "format": "woff2"
    }
  },
  "appearance": {
    "default": "light"
  },
  "background": {
    "decoration": "gradient"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "API Documentation",
        "openapi": "openapi.yaml",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["api-reference/introduction", "api-reference/authentication"]
          }
        ]
      },
      {
        "tab": "Changelog",
        "pages": ["changelog"]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Status",
          "href": "https://status.kajabi.com/",
          "icon": "signal"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://kajabi.com/"
  },
  "navbar": {
    "links": [
      {
        "label": "Login",
        "href": "https://app.kajabi.com/login"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started for Free",
      "href": "https://kajabi.com/?gad_source=1#"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "facebook": "https://www.facebook.com/kajabi",
      "x": "https://www.x.com/kajabi",
      "instagram": "https://www.instagram.com/kajabi",
      "linkedin": "https://www.linkedin.com/company/kajabi/",
      "youtube": "https://www.youtube.com/@kajabiapp"
    }
  }
}
